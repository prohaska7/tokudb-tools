# ----------------------------------------------------------------------
# Test of JSN_VALID function.
# ----------------------------------------------------------------------

# Table - Json string column - non-utf8 character set
create table latin_t (c varchar(20)) CHARACTER SET 'latin1';
insert into latin_t values ('123');
select JSN_VALID(c) from latin_t;
JSN_VALID(c)
1
drop table latin_t;

# Table - Json string column - utf-8, NULL
Note: 'utf8' is a subset of internal 'utf8mb4'

create table utf8_t (c varchar(20)) CHARACTER SET 'utf8';
insert into utf8_t values (NULL);
# Expect NULL:
select JSN_VALID(c) from utf8_t;
JSN_VALID(c)
NULL
delete from utf8_t;

# Table - Json string column - utf-8, valid
insert into utf8_t values ('[123]');
select JSN_VALID(c) from utf8_t;
JSN_VALID(c)
1
delete from utf8_t;

# Table - Json string column - utf-8, non-utf8
insert into utf8_t values ('[123');
expect 0 (false)
select JSN_VALID(c) from utf8_t;
JSN_VALID(c)
0
delete from utf8_t;

# Table - Try to extract JSON from TIMESTAMP column
ALTER TABLE utf8_t ADD d TIMESTAMP;
# Should give false; not string or JSON type
# and we do not convert automatically from TIMESTAMP to JSON
insert into utf8_t values (NULL, '2014-11-25 18:00');
select JSN_VALID(d) from utf8_t;
JSN_VALID(d)
0
# Explicit cast to a character data type
# allows MySQL to parse this is a JSON text
# The string isn't a legal JSON document, tho, so not valid.
select JSN_VALID(CAST(d as CHAR)) from utf8_t;
JSN_VALID(CAST(d as CHAR))
0
# Should give true
select JSN_VALID(CONCAT( CONCAT('"', CAST(d as CHAR)), '"')) from utf8_t;
JSN_VALID(CONCAT( CONCAT('"', CAST(d as CHAR)), '"'))
1
delete from utf8_t;
drop table utf8_t;

# Table - JSON type; should give true by definition
create table json_t(t json);
insert into json_t values ('[123]');
select jsn_VALID(t) from json_t;
jsn_VALID(t)
1

# String literal - valid JSON
select JSN_VALID('123');
JSN_VALID('123')
1
select JSN_VALID('"123"');
JSN_VALID('"123"')
1
select JSN_VALID('true');
JSN_VALID('true')
1
select JSN_VALID('false');
JSN_VALID('false')
1
select JSN_VALID('null');
JSN_VALID('null')
1
select JSN_VALID('{"address": "Trondheim"}');
JSN_VALID('{"address": "Trondheim"}')
1

# String literal - not valid JSON
select JSN_VALID('12 3');
JSN_VALID('12 3')
0

# String literal not in UTF-8
set names 'ascii';
select JSN_VALID('123');
JSN_VALID('123')
1
set names 'utf8';

# Json expression
select JSN_VALID(cast('[123]' as JSON ));
JSN_VALID(cast('[123]' as JSON ))
1

# Json expression NULL
select JSN_VALID(cast(NULL as JSON ));
JSN_VALID(cast(NULL as JSON ))
NULL

# Bare NULL
select JSN_VALID( NULL );
JSN_VALID( NULL )
NULL

# Function result - string
select JSN_VALID( UPPER('"abc"') );
JSN_VALID( UPPER('"abc"') )
1
set names 'latin1';

# Function result - string
select JSN_VALID( UPPER('"abc"') );
JSN_VALID( UPPER('"abc"') )
1
set names 'utf8';

# Function result - date, not valid as JSON without CAST
select JSN_VALID( CAST('2015-01-15' AS DATE) );
JSN_VALID( CAST('2015-01-15' AS DATE) )
0

# The date string doesn't parse as JSON text, so wrong:
select JSN_VALID( CAST(CAST('2015-01-15' AS DATE) as CHAR CHARACTER SET 'utf8') );
JSN_VALID( CAST(CAST('2015-01-15' AS DATE) as CHAR CHARACTER SET 'utf8') )
0
# OK, though:
select JSN_VALID( CAST(CURDATE() as JSON) );
JSN_VALID( CAST(CURDATE() as JSON) )
1

# Function result - NULL
select JSN_VALID( UPPER(NULL) );
JSN_VALID( UPPER(NULL) )
NULL
select JSN_VALID( UPPER(CAST(NULL as CHAR)) );
JSN_VALID( UPPER(CAST(NULL as CHAR)) )
NULL

# Function result - JSON
select JSN_VALID( JSN_ARRAY(t, t) ) from json_t;
JSN_VALID( JSN_ARRAY(t, t) )
1
drop table json_t;
SELECT JSN_VALID( '{ "firstName" : "Fred", "lastName" : "Flintstone" }' );
JSN_VALID( '{ "firstName" : "Fred", "lastName" : "Flintstone" }' )
1
SELECT JSN_VALID( '3' );
JSN_VALID( '3' )
1
SELECT JSN_VALID( null );
JSN_VALID( null )
NULL
