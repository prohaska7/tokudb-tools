# Test of JSN_CONTAINS
# ----------------------------------------------------------------------
# should give NULL
select jsn_contains(NULL, NULL);
jsn_contains(NULL, NULL)
NULL
select jsn_contains(cast('{"a": 1, "b": 2}' as json), NULL);
jsn_contains(cast('{"a": 1, "b": 2}' as json), NULL)
NULL
select jsn_contains(NULL, cast('null' as json));
jsn_contains(NULL, cast('null' as json))
NULL
select jsn_contains(cast('[1]' as json), cast('[1]' as json), NULL);
jsn_contains(cast('[1]' as json), cast('[1]' as json), NULL)
NULL
# should give 0:
select jsn_contains(cast(3.14 as json), cast(3 as json));
jsn_contains(cast(3.14 as json), cast(3 as json))
0
# should give 0: not at top level
select jsn_contains(cast('{"a": {"b": 7}}' as json), cast('{"b": 7}' as json));
jsn_contains(cast('{"a": {"b": 7}}' as json), cast('{"b": 7}' as json))
0
# but path argument will fix it:
select jsn_contains(cast('{"a": {"b": 7}}' as json), cast('{"b": 7}' as json), '$.a');
jsn_contains(cast('{"a": {"b": 7}}' as json), cast('{"b": 7}' as json), '$.a')
1
# but arrays "introspect"
select jsn_contains(cast('[1,[2.0, 3.0]]' as json), cast('[2.0]' as json));
jsn_contains(cast('[1,[2.0, 3.0]]' as json), cast('[2.0]' as json))
1
select jsn_contains(cast('[1, 2, [3, [4, 5]], 6, 7]' as json), cast('5' as json));
jsn_contains(cast('[1, 2, [3, [4, 5]], 6, 7]' as json), cast('5' as json))
1
# should give 0: just a key
select jsn_contains(cast('{"a": 1, "b": 2}' as json), cast('"a"' as json));
jsn_contains(cast('{"a": 1, "b": 2}' as json), cast('"a"' as json))
0
# should give 0: one candidate element doesn't match
select jsn_contains(cast('[1]' as json), cast('[1,2]' as json));
jsn_contains(cast('[1]' as json), cast('[1,2]' as json))
0
# should all give 1
select jsn_contains(cast('null' as json), cast('null' as json));
jsn_contains(cast('null' as json), cast('null' as json))
1
# simple object subset
select jsn_contains(cast('{"a": 1, "b": 2}' as json), cast( '{"a": 1}' as json));
jsn_contains(cast('{"a": 1, "b": 2}' as json), cast( '{"a": 1}' as json))
1
# simple vector subset
select jsn_contains(cast('[1, 2, 3]' as json), cast('[1, 3]' as json));
jsn_contains(cast('[1, 2, 3]' as json), cast('[1, 3]' as json))
1
# auto-wrap, should give 1
select jsn_contains(cast('[1, 2, 3]' as json), cast(3 as json));
jsn_contains(cast('[1, 2, 3]' as json), cast(3 as json))
1
# ok even with nested cast off elements
select jsn_contains(cast('{"person": {"id": 1, "country": "norway"}}' as json),
cast('{"person": {"country": "norway"}}' as json));
jsn_contains(cast('{"person": {"id": 1, "country": "norway"}}' as json),
cast('{"person": {"country": "norway"}}' as json))
1
# vector reordering and duplicates is ok
select jsn_contains(cast('[1,3,5]' as json), cast('[5,3,1,5]' as json));
jsn_contains(cast('[1,3,5]' as json), cast('[5,3,1,5]' as json))
1
# ok even with more elts in candidate than in doc
select jsn_contains(cast('[{"b": 4, "a":7}]' as json), cast('[{"a":7},{"b":4}]' as json));
jsn_contains(cast('[{"b": 4, "a":7}]' as json), cast('[{"a":7},{"b":4}]' as json))
